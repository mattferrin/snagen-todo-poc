{
  "name": "snagen",
  "version": "0.0.0",
  "license": "MIT",
  "bin": "dist/cli.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "tsc",
    "fsl": "fsl",
    "jscpd": "jscpd --threshold=1 --ignore=**/*.test.tsx --reporters=html ./source",
    "lint:": "xo",
    "pretest": "npm run build",
    "start": "ts-node source/cli.tsx",
    "test:w": "jest --watch",
    "test": "jest",
    "tsc:w": "tsc -w",
    "tsc": "tsc"
  },
  "files": [
    "dist/cli.js"
  ],
  "dependencies": {
    "ink": "^3.2.0",
    "meow": "^9.0.0",
    "react": "^17.0.2"
  },
  "ava": {
    "typescript": {
      "extensions": [
        "tsx"
      ],
      "rewritePaths": {
        "source/": "dist/"
      },
      "compile": false
    }
  },
  "xo": {
    "extends": "xo-react",
    "rules": {
      "react/prop-types": "off"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@clowd/fsl": "^1.0.5",
    "@sindresorhus/tsconfig": "^3.0.1",
    "@types/jest": "^29.2.4",
    "@types/react": "^18.0.26",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.3.1",
    "chalk": "^4.1.2",
    "eslint-config-xo-react": "^0.27.0",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.3.1",
    "jscpd": "^3.5.3",
    "react-test-renderer": "^17.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "xo": "^0.39.1"
  }
}
